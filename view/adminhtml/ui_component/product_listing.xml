<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <container name="listing_top">
        <massaction name="listing_massaction">
            <action name="mass_duplicate">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">mass_duplicate</item>
                        <item name="label" xsi:type="string" translate="true">Duplicate</item>
                        <item name="url" xsi:type="string">atwix_mass_duplicate/product/massDuplicate</item>
                    </item>
                </argument>
            </action>
        </massaction>
    </container>
</listing>